  "musica": {
    "title": "Comandos de Música",
    "content": [
      {
        "header": "**>play** *<link do youtube/nome>*",
        "desc": "Toca a música do YouTube com o link ou nome fornecido. Esse comando também suporta link de playlists."
      },
      {
        "header": "**>queue**",
        "desc": "Mostra a fila de reprodução"
      },
      {
        "header": "**>skip**",
        "desc": "Pula a música atual para a próxima faixa da lista de reprodução."
      },
      {
        "header": "**>pause**",
        "desc": "Pausa a música"
      },
      {
        "header": "**>resume**",
        "desc": "Despausa a música"
      },
      {
        "header": "**>move** *<posição atual>* *<posição de destino>*",
        "desc": "Move uma música da posição atual para a posição de destino na fila de reprodução."
      },
      {
        "header": "**>remove** *<posição>*",
        "desc": "Remove a música correspondente à posição da fila de reprodução."
      },
      {
        "header": "**>shuffle**",
        "desc": "Embaralha a fila de reprodução"
      }
    ]
